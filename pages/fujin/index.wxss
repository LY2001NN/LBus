page {
    background: #f5f5f5;
}

.sousuolist {
    background: #fff;
    border-bottom: 1px solid #f5f5f5;
    font-size: 14px;
    line-height: 25px;
    margin-top: 0;
    padding: 10px 0;
    width: 100%;
}

.sousuolist .houmian {
    color: #555;
    padding-left: 5%;
    position: relative;
    width: 90%;
}

.sousuolist .houmian .houmian_title {
    color: #333;
    font-size: 14px;
}

.sousuolist .houmian .houmian_dis {
    color: #888;
    font-size: 12px;
    position: absolute;
    right: 0;
    top: 0;
}

.sousuolist .houmian .houmian_text {
    color: #888;
    font-size: 12px;
    overflow: hidden;
    text-overflow: ellipsis;
    white-space: nowrap;
}

.lijiqiehuan {
    background: #08c3a8;
    border-radius: 5px;
    color: #fff;
    font-size: 13px;
    height: 30px;
    line-height: 30px;
    margin-left: 35%;
    margin-top: 10px;
    text-align: center;
    width: 30%;
}